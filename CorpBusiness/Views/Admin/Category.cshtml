 @model IEnumerable<CorpBusiness.Models.Category>
@{
    ViewBag.Title = "Category";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<!DOCTYPE html>

<html>
<head>
    <title>Category</title>
</head>
<body>
    <script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <table style=" width:90%; font-size:9px; float:right;" >
        <tr>
            <th></th>
            <th>
                Category
            </th>
            <th>
                Sub Category
            </th>
             
        </tr>
    
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink("Edit", "EditCat", new { id = item.Id }) |
                @Html.ActionLink("Details", "DetailsCat", new { id = item.Id }) |
                @Html.ActionLink("Delete", "DeleteCat", new { id = item.Id })
            </td>
            <td>
                @item.Categories
            </td>
            <td>
                @item.Sub_Categories
            </td>
                 </tr>
    }
    
    </table>
   
    <div>
        @Html.ActionLink("Back to Home","Users")
    </div>
</body>
</html>



